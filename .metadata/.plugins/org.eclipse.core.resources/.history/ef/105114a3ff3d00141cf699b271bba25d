
public class MathProgram {
	
	public static void main (String[] args) {
		long dis = 1989;
		long dat = 1590;
		long answer = euclid(dis, dat);
		System.out.println("The greatest common divider of " + dis +
				" and " + dat + "is" + answer);
	}
	
	// Euclid’s algorithm for computing the greatest common divisor
	public static long euclid ( long m, long n ) {
		System.out.print("Sequence:");
		while (n != 0) {
			long rem = m % n;
			m = n;
			n = rem;
			System.out.print(n + " ");
		}
		System.out.println();
		return m;
	}
}
