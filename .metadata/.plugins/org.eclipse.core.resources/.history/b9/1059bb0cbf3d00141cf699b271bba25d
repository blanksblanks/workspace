
public class Exercise2_7 {
	
	public static void main(String[] args) {
		
		int[] numbers = {1, 2, 4, 8, 16, 32, 64, 128, 256, 512, 1024, 2048};
		TimeInterval time = new TimeInterval();
		
		for (int i = 0; i < numbers.length; i++) {
			time.startTiming();
			int sum1 = sumOne(numbers[i]);
			time.endTiming();
			double runtime1 = time.getElapsedTime();
			int sum2 = sumTwo(numbers[i]);
			int sum3 = sumThree(numbers[i]);
			int sum4 = sumFour(numbers[i]);
			int sum5 = sumFive(numbers[i]);
			int sum6 = sumSix(numbers[i]);

			System.out.println("The first sum of " + numbers[i] + " is " + sum1);
			System.out.println("The second sum of " + numbers[i] + " is " + sum2);
			System.out.println("The third sum of " + numbers[i] + " is " + sum3);
			System.out.println("The fourth of " + numbers[i] + " is " + sum4);
			System.out.println("The fifth sum of " + numbers[i] + " is " + sum5);
			System.out.println("The sixth sum of " + numbers[i] + " is " + sum6);
		}
	
		
		// TODO: Implement the code in Java, and give the running time for several values of N.
		// TODO: Compare your analysis with the actual running times.
	}
	
	public static int sumOne(int n) {
		int sum = 0;
		for ( int i = 0; i < n; i++ ) {
			sum++;
		}
		return sum;
	}
	
	public static int sumTwo(int n) {
		int sum = 0;
		for ( int i = 0; i < n; i++ ) {
			for ( int j = 0; j < n; j++ ) {
				sum++;
			}
		}
		return sum;
	}
	
	public static int sumThree(int n) {
		int sum = 0;
        for ( int i = 0; i < n; i++ ) {
            for ( int j = 0; j < n * n; j++ ) {
                sum++;
            }
        }
        return sum;
	}

	public static int sumFour(int n) {
		int sum = 0;
        for ( int i = 0; i < n; i++ ) {
            for ( int j = 0; j < i; j++ ) {
                sum++;
            }
        }
        return sum;
	}
	
	public static int sumFive(int n) {
		int sum = 0;
		for ( int i = 0; i < n; i++ ) {
			for ( int j = 0; j < (i * i); j++ ) {
		    	 for (int k = 0; k < j; k++ ) {
		                    	   		sum++;
		    	 }
			}
		}
		return sum;
	}
		
	public static int sumSix(int n) {
		int sum = 0;
		for ( int i = 1; i < n; i++ ) {
			for ( int j = 1; j < i * i; j++ ) {
				if ( j % i == 0 ) {
					for ( int k = 0; k < j; k++ ) {
		                              sum++;
					}
				}
			}
		}
		return sum;
	}
}
